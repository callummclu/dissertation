{
  "roots":[
    "src"
  ],
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "js", "json", "node"],
    "testPathIgnorePatterns" : [
      "__tests__/assets","__tests__/helpers"
    ],
    "testTimeout": 60000
  }
  